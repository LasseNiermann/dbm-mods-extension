{
    "name": "dbm-mods-extensions",
    "displayName": "DBM Mods Extensions",
    "description": "Helps with some things to improve development of DBM Mods!",
    "version": "0.0.1",
    "publisher": "generalwrex",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:dbmmodsextensions.loadMod"
    ],
    "main": "./dbmmodsextensions",
